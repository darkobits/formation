{
  "tags": {
    "allowUnknownTags": false,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": [
      "package.json",
      "src/formation"
    ],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "template": "src/jsdoc-theme"
  }
}
