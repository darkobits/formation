  ],
  "engines": {
    "npm": ">=3.0.0",
    "node": ">=6.x"
  },
  "scripts": {
    "start": "npm run clean && webpack-dev-server --env.local",
    "test": "karma start --env.test",
    "test:watch": "npm test -- --auto-watch --no-single-run",
    "lint:js": "eslint .",
    "lint:sass": "sass-lint --verbose --no-exit",
    "lint": "npm-run-all --parallel lint:*",
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --env.dist --progress",
    "bundle-stats": "npm run build -- --env.stats",
    "docs:build": "rimraf docs && jsdoc --configure jsdoc.conf.json --destination docs --recurse --verbose",
    "precommit": "npm run lint"
  },
  "peerDependencies": {
    "babel-polyfill": "6.x"
  },
  "dependencies": {
    "angular": "1.5.x",
    "angular-messages": "1.5.x",
    "ramda": "0.23.x"
  },
  "devDependencies": {
    "autoprefixer": "6.3.x",
    "babel-core": "6.x",
    "babel-loader": "6.x",
    "babel-plugin-istanbul": "2.0.x",
    "babel-polyfill": "6.x",
    "babel-preset-es2015": "6.x",
    "babel-runtime": "6.x",
    "css-loader": "0.25.x",
    "eslint": "3.12.x",
    "eslint-loader": "1.5.x",
    "eslint-plugin-angular": "1.3.x",
    "extract-text-webpack-plugin": "2.0.0-beta.1",
    "file-loader": "0.9.x",
    "husky": "0.13.x",
    "jasmine-core": "2.4.x",
    "jasmine-fail-fast": "2.0.x",
    "jasmine-reporters": "2.2.x",
    "jsdoc": "3.4.x",
    "karma": "1.1.x",
    "karma-coverage": "1.1.x",
    "karma-jasmine": "1.0.x",
    "karma-phantomjs-launcher": "1.x",
    "karma-webpack": "1.8.x",
    "ng-annotate-loader": "0.1.x",
    "ngtemplate-loader": "1.3.x",
    "node-sass": "3.8.x",
    "npm-run-all": "2.2.x",
    "phantomjs-prebuilt": "2.1.x",
    "postcss-loader": "0.9.x",
    "raw-loader": "0.5.x",
    "rimraf": "2.5.x",
    "sass-lint": "1.10.x",
    "sass-loader": "4.0.x",
    "sasslint-webpack-plugin": "1.0.x",
    "style-loader": "0.13.x",
    "svg-sprite-loader": "0.1.x",
    "svg-url-loader": "1.1.x",
    "url-loader": "0.5.x",
    "webpack": "2.x",
    "webpack-visualizer-plugin": "0.1.x"
  }
