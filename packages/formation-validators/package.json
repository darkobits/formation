{
  "name": "@darkobits/formation-validators",
  "version": "1.0.0-beta.6",
  "license": "WTFPL",
  "contributors": [
    {
      "name": "Joshua Martin",
      "email": "joshua@frontlawn.net"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/darkobits/formation.git"
  },
  "engines": {
    "npm": ">=4.x",
    "node": ">=6.x"
  },
  "main": "dist/validators.js",
  "browser": "dist/validators.min.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "xo",
    "test": "jest",
    "test:watch": "npm t -- --watch",
    "test:coverage": "npm t -- --coverage",
    "prebuild": "run-p clean lint",
    "build:commonjs": "babel src --out-dir dist",
    "build:browser": "webpack --env.dist --progress",
    "build": "run-p build:commonjs build:browser",
    "build:watch": "watch \"npm run build\" ./src/",
    "bundle-stats": "webpack --env.stats --env.dist",
    "prepublish": "run-p test build"
  },
  "xo": false,
  "peerDependencies": {
    "babel-polyfill": "6.x"
  },
  "dependencies": {
    "@darkobits/formation": "^1.0.0-beta.6",
    "ramda": "0.24.x"
  },
  "devDependencies": {
    "@darkobits/unity": "1.x",
    "babel-cli": "6.x",
    "babel-jest": "20.x",
    "babel-loader": "6.x",
    "babel-preset-env": "1.4.x",
    "eslint": "4.x",
    "jest": "20.x",
    "npm-run-all": "4.x",
    "rimraf": "2.6.x",
    "watch": "1.x",
    "webpack": "3.x",
    "xo": "0.18.x"
  }
}
