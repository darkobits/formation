{
  "moduleDirectories": [
    "<rootDir>",
    "<rootDir>/src",
    "<rootDir>/tests",
    "node_modules"
  ],
  "testPathIgnorePatterns": [
    "/dist/"
  ],
  "coveragePathIgnorePatterns": [
    "/dist/",
    "/tests/"
  ],
  "coverageThreshold": {
    "global": {
      "statements": 50,
      "branches": 50,
      "functions": 50,
      "lines": 50
    }
  }
}
